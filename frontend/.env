# Frontend Environment Variables
# Create this file in your React project root (where package.json is)

# API Configuration
VITE_API_URL=http://localhost:8000/api

# App Configuration
REACT_APP_NAME=Smart Education Platform
REACT_APP_VERSION=1.0.0

# Feature Flags (optional)
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true

# External Services (add as needed)
# REACT_APP_GOOGLE_MAPS_KEY=your-google-maps-key
# REACT_APP_STRIPE_PUBLIC_KEY=your-stripe-public-key
# REACT_APP_CLOUDINARY_URL=your-cloudinary-url

# Environment
REACT_APP_ENV=development
